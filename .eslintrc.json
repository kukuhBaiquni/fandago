{
  "env": {
    "browser": false,
    "commonjs": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint:recommended"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "semi": ["error", "never"],
    "indent": ["warn", 2],
    "quotes": ["warn", "single", { "avoidEscape": true }],
    "no-use-before-define": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": ["error", "windows"],
    "camelcase": "off"
  }
}
